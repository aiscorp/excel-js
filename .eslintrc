{
  "parser": "babel-eslint",
  "parserOptions": {
    "requireConfigFile": false
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "no-unused-vars": "off",
    "no-trailing-spaces": "off",
    "indent": "off",
    "no-empty": "off",
    "linebreak-style": ["error", "windows"],
    "max-len": "warn",
    "padded-blocks": "warn",
    "no-multiple-empty-lines": "warn"
  },
  "extends": ["eslint:recommended", "google"]
}

